<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            prefHeight="400.0" prefWidth="600.0">
    <HBox alignment="CENTER_LEFT" layoutX="65.0" layoutY="99.0"
          prefHeight="356.6666666666667" prefWidth="583.3333333333334"
          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <children>
            <VBox alignment="TOP_CENTER" prefHeight="384.0" prefWidth="396.0">
                <children>
                    <Label text="%scene.settings.account.currentUser" textFill="#c4c4c4">
                        <font>
                            <Font name="Arial Bold" size="12.0"/>
                        </font>
                    </Label>
                    <VBox alignment="CENTER" prefHeight="338.0" prefWidth="363.0"
                          VBox.vgrow="ALWAYS">
                        <children>
                            <VBox alignment="CENTER_LEFT" prefHeight="48.0"
                                  prefWidth="240.0">
                                <children>
                                    <Label text="%scene.memberDetails.fullName">
                                        <font>
                                            <Font name="Arial Bold" size="12.0"/>
                                        </font>
                                    </Label>
                                    <HBox prefHeight="25.0" prefWidth="233.0" spacing="5.0">
                                        <children>
                                            <TextField prefHeight="25.0" prefWidth="198.0"
                                                       HBox.hgrow="ALWAYS"/>
                                            <Button mnemonicParsing="false"
                                                    prefHeight="25.0" prefWidth="25.0">
                                                <graphic>
                                                    <ImageView fitHeight="15.0"
                                                               fitWidth="15.0"
                                                               pickOnBounds="true"
                                                               preserveRatio="true"/>
                                                </graphic>
                                            </Button>
                                        </children>
                                    </HBox>
                                </children>
                            </VBox>
                            <VBox alignment="CENTER_LEFT" layoutX="80.0" layoutY="10.0"
                                  prefHeight="48.0" prefWidth="240.0">
                                <children>
                                    <Label text="%scene.settings.account.login">
                                        <font>
                                            <Font name="Arial Bold" size="12.0"/>
                                        </font>
                                    </Label>
                                    <HBox prefHeight="25.0" prefWidth="233.0" spacing="5.0">
                                        <children>
                                            <TextField prefHeight="25.0" prefWidth="198.0"
                                                       HBox.hgrow="ALWAYS"/>
                                            <Button mnemonicParsing="false"
                                                    prefHeight="25.0" prefWidth="25.0">
                                                <graphic>
                                                    <ImageView fitHeight="15.0"
                                                               fitWidth="15.0"
                                                               pickOnBounds="true"
                                                               preserveRatio="true"/>
                                                </graphic>
                                            </Button>
                                        </children>
                                    </HBox>
                                </children>
                            </VBox>
                            <VBox alignment="CENTER_LEFT" layoutX="80.0" layoutY="58.0"
                                  prefHeight="48.0" prefWidth="240.0">
                                <children>
                                    <Label text="%scene.login.password">
                                        <font>
                                            <Font name="Arial Bold" size="12.0"/>
                                        </font>
                                    </Label>
                                    <HBox prefHeight="25.0" prefWidth="233.0" spacing="5.0">
                                        <children>
                                            <PasswordField HBox.hgrow="ALWAYS"/>
                                            <Button mnemonicParsing="false"
                                                    prefHeight="25.0" prefWidth="25.0">
                                                <graphic>
                                                    <ImageView fitHeight="15.0"
                                                               fitWidth="15.0"
                                                               pickOnBounds="true"
                                                               preserveRatio="true"/>
                                                </graphic>
                                            </Button>
                                            <Button layoutX="211.0" layoutY="10.0"
                                                    mnemonicParsing="false"
                                                    prefHeight="25.0" prefWidth="25.0">
                                                <graphic>
                                                    <ImageView fitHeight="15.0"
                                                               fitWidth="15.0"
                                                               pickOnBounds="true"
                                                               preserveRatio="true"/>
                                                </graphic>
                                            </Button>
                                        </children>
                                    </HBox>
                                </children>
                            </VBox>
                            <Region VBox.vgrow="ALWAYS"/>
                        </children>
                        <padding>
                            <Insets left="50.0" right="50.0"/>
                        </padding>
                    </VBox>
                </children>
                <padding>
                    <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
                </padding>
            </VBox>
            <Region prefHeight="384.0" prefWidth="0.0" HBox.hgrow="ALWAYS"/>
            <VBox alignment="TOP_CENTER" layoutX="10.0" layoutY="10.0" prefHeight="384.0"
                  prefWidth="205.0" spacing="8.0">
                <children>
                    <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="247.0"
                            text="%scene.settings.account.newAccount"/>
                    <Region VBox.vgrow="ALWAYS"/>
                    <Button layoutX="31.0" layoutY="20.0" mnemonicParsing="false"
                            prefHeight="25.0" prefWidth="209.0"
                            text="%scene.settings.account.deleteAccount" textFill="RED"/>
                </children>
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
            </VBox>
        </children>
    </HBox>
</AnchorPane>
